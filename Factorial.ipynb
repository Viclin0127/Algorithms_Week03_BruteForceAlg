    "def FAC(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return FAC(n-1) * n\n",
    "\n",
    "def FAC2(n):\n",
    "    result = 1\n",
    "    while n > 0:\n",
    "        result = result * n\n",
    "        n = n-1\n",
    "    return result\n",
    "\n",
    "def test():\n",
    "    print(FAC(4))\n",
    "    print(FAC2(3))\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    test()"
   ]
